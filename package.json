{
	"scripts": {
		"build": "rm -rf ./dist && parcel build --target k6 src/test/**.ts && mkdir ./dist/reports && mkdir ./dist/source",
		"clean": "rm -rf ./dist && rm -rf ./parcel-cache",
		"test1": "k6 run dist/crocodiles.create.js",
		"test": "k6 run src/test.ts",
		"test2": "k6 run \"$npm_config_file1\"",
		"lint": "eslint .",
		"prettier": "prettier --write .",
		"report": "node ./src/utils/report.js"
	},
	"targets": {
		"k6": {
			"context": "node",
			"includeNodeModules": {
				"k6": false
			}
		}
	},
	"devDependencies": {
		"@types/faker": "^4.1.12",
		"@types/k6": "0.32.0",
		"@types/node": "^15.12.2",
		"@typescript-eslint/eslint-plugin": "4.26.1",
		"@typescript-eslint/parser": "4.26.1",
		"eslint": "7.28.0",
		"faker": "^4.1.0",
		"k6-html-reporter": "0.0.6",
		"parcel": "2.0.0-beta.1",
		"prettier": "2.3.1",
		"ts-node": "^10.0.0",
		"typescript": "4.3.2"
	},
	"dependencies": {
		"fs-extra": "^10.0.0",
		"k6": "0.0.0"
	}
}
